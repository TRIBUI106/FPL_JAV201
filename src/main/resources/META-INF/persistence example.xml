<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_2.xsd"
             version="3.2">
    <persistence-unit name="yeumeent_dihoc">
        <properties>
            <property name="jakarta.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
            <property name="jakarta.persistence.jdbc.url" value="value"/>
            <property name="jakarta.persistence.jdbc.user" value="value"/>
            <property name="jakarta.persistence.jdbc.password" value="value"/>
            <property name="jakarta.persistence.sql-load-script-source" value="true"/>
        </properties>
    </persistence-unit>
</persistence>
